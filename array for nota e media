#include <stdio.h>

int main()
{
 float nota[4] = {0,0,0,0};
 int i;
 float media;
 float soma = 0;
 
    for(i = 1; i < 5; i++){
        printf("digite %dº nota: ", i);
        scanf("%f", &nota[i]);
        
        soma += nota[i];
    }
    
    media = soma /4;
    printf("a media é de %.1f: ", media);
    
   
return 0;
}
